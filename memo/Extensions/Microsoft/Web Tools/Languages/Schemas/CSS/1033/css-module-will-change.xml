<CssModule name="Will Change" standard-reference="http://www.w3.org/TR/css-will-change/" status="WD" pub-date="2014-04-29">

    <CssProperty _locID="will-change"
                 _locAttrData="description"
                 version="3.0"
                 browsers="C,FF,O"
                 restriction="enum, identifier"
                 syntax="div { $(name): transform; }"
                 description="Provides a rendering hint to the user agent, stating what kinds of changes the author expects to perform on the element"
                 standard-reference="http://www.w3.org/TR/css-will-change/#will-change">

        <entry _locID="will-change-auto"
               _locAttrData="description"
               value="auto"
               description="Expresses no particular intent; the user agent should apply whatever heuristics and optimizations it normally does." />
        <entry _locID="will-change-scroll-position"
               _locAttrData="description"
               value="scroll-position"
               description="Indicates that the author expects to animate or change the scroll position of the element in the near future." />
        <entry _locID="will-change-contents"
               _locAttrData="description"
               value="contents"
               description="Indicates that the author expects to animate or change something about the element’s contents in the near future." />
    </CssProperty>
</CssModule>