    <entry value="last baseline" />
    <entry value="space-between" />
    <entry value="space-around" />
    <entry value="space-evenly" />
    <entry value="stretch" />
    <entry value="safe center" />
    <entry value="unsafe center" />
    <entry value="center" />
    <entry value="start" />
    <entry value="end" />
    <entry value="flex-start" />
    <entry value="flex-end" />
  </CssProperty>

  <CssProperty _locID="justify-self"
               _locAttrData="description"
               version="3.0"
               browsers="FF,IE"
               restriction="enum"
               description="Justifies the box (as the alignment subject) within its containing block (as the alignment container) along the inline/row/main axis of the alignment container."
               syntax="div { $(name): stretch; }"
               standard-reference="http://www.w3.org/TR/css-align-3/#propdef-justify-self">

    <entry value="auto" />
