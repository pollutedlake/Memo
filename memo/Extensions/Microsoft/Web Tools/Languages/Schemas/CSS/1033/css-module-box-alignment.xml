<CssModule name="Box Alignment" standard-reference="http://www.w3.org/TR/css-align-3/" status="WD" pub-date="2017-09-06">

  <CssProperty _locID="justify-content"
               _locAttrData="description"
               version="3.0"
               browsers="C29,FF28,IE11,O17,S9"
               restriction="enum"
               description="Aligns the contents of the box as a whole (as the alignment subject) within the box itself (as the alignment container) along the inline/row/main axis of the box."
               syntax="div { $(name): flex-start; }"
               standard-reference="http://www.w3.org/TR/css-align-3/#propdef-justify-content">

    <entry value="normal" />
    <entry value="left" />
    <entry value="right" />
    <entry value="space-between" />
    <entry value="space-around" />
    <entry value="space-evenly" />
    <entry value="stretch" />
    <entry value="safe center" />
    <entry value="unsafe center" />
    <entry value="center" />
    <entry value="start" />
    <entry value="end" />
    <entry value="flex-start" />
    <entry value="flex-end" />
  </CssProperty>

  <CssProperty _locID="align-content"
               _locAttrData="description"
               version="3.0"
               browsers="C21,FF28,IE11,O12.1,S9"
               restriction="enum"
               description="Aligns the contents of the box as a whole (as the alignment subject) within the box itself (as the alignment container) along the block/column/cross axis of the box."
               syntax="div { $(name): stretch; }"
               standard-reference="http://www.w3.org/TR/css-align-3/#propdef-align-content">

    <entry value="normal" />
    <entry value="baseline" />
