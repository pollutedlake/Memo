<CssModule name="Values" standard-reference="http://www.w3.org/TR/css3-values/" status="WD" pub-date="2011-09-06">

  <CssPropertyValue type="all">
    <entry _locID="all-val_inherit"
           _locAttrData="description"
           value="inherit"
           version="2.1"
           description="Causes the element to gain its parent's computed value for the property." />
    <entry _locID="all-val_initial"
           _locAttrData="description"
           value="initial"
           version="3.0"
           browsers="none"
           description="The default value of any property." />
    <entry _locID="all-val_unset"
           _locAttrData="description"
           value="unset"
           version="3.0"
           browsers="none"
           description="If the cascaded value of a property is the 'unset' keyword, then if it is an inherited property, this is treated as 'inherit', and if it is not, this is treated as 'initial'." />
  </CssPropertyValue>

  <CssProperty _locID="all"
              _locAttrData="description"
              version="3.0"
              restriction="enum"
              description="The 'all' property is a shorthand that resets all CSS properties except 'direction' and 'unicode-bidi'."
              syntax="div { $(name): initial; }"
              standard-reference="http://www.w3.org/TR/css-cascade-3/#all-shorthand" />

</CssModule>
