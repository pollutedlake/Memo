<CssModule name="Regions" standard-reference="http://dev.w3.org/csswg/css-regions/" status="WD" pub-date="2013-10-15">

  <CssProperty name="break-after">
    <entry value="avoid-region"/>
    <entry value="region"/>
  </CssProperty>

  <CssProperty name="break-before">
    <entry value="avoid-region"/>
    <entry value="region"/>
  </CssProperty>

  <CssProperty name="break-inside">
    <entry value="avoid-region"/>
  </CssProperty>

  <CssProperty _locID="flow-from" _locAttrData="description" version="3.0" browsers="none" restriction="identifier" syntax="div { $(name): identifier; }" description="데이터 원본으로부터 콘텐츠 흐름을 받아들이는 문서에서 연결된 프레임 컨테이너를 식별하는 값을 가져오거나 설정합니다." standard-reference="http://www.w3.org/TR/css3-regions/#flow-from">

    <entry value="none"/>
  </CssProperty>

  <CssProperty _locID="flow-into" _locAttrData="description" version="3.0" browsers="none" restriction="identifier" syntax="div { $(name): identifier; }" description="연결된 프레임 데이터 원본으로 사용되는 문서에서 iframe 컨테이너를 식별하는 값을 가져오거나 설정합니다." standard-reference="http://www.w3.org/TR/css3-regions/#flow-into">

    <entry value="none"/>
  </CssProperty>

  <CssProperty _locID="region-fragment" _locAttrData="description" version="3.0" browsers="none" restriction="enum" syntax="article { $(name): break; }" description="'region-fragment' 속성은 명명된 흐름과 연결된 마지막 영역의 동작을 제어합니다." standard-reference="http://dev.w3.org/csswg/css-regions/#region-fragment">

    <entry _locID="region-fragment-auto" _locAttrData="description" value="auto" description="일반 콘텐츠 상자에 있는 것처럼 콘텐츠가 이동합니다."/>
    <entry _locID="region-fragment-break" _locAttrData="description" value="break" description="콘텐츠가 CSS 영역 내에 맞으면 이 속성이 아무런 영향도 주지 않습니다."/>
  </CssProperty>

</CssModule>
