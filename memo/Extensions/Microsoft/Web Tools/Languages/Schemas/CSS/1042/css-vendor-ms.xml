<CssModule name="Microsoft Extensions" standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx" pub-date="2012-02-23">

  <CssAtDirective name="-ms-keyframes" version="3.0" browsers="none" description="Defines set of animation key frames." obsolete="Use @keyframes instead"/>

  <CssAtDirective name="-ms-viewport" version="3.0" browsers="IE10" include-root-properties="false" include-root-directives="false" syntax="@$(name) { width: device-width; }" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh868813.aspx">

    <CssProperty ref="width">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>

    <CssProperty ref="height">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>

    <CssProperty ref="max-width">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>

    <CssProperty ref="min-width">
      <entry value="device-width"/>
      <entry value="device-height"/>
    </CssProperty>

    <CssProperty name="zoom" restriction="number, percentage">
      <entry value="auto"/>
    </CssProperty>

    <CssProperty name="max-zoom" restriction="number, percentage">
    </CssProperty>

    <CssProperty name="min-zoom" restriction="number, percentage">
    </CssProperty>

    <CssProperty name="user-zoom">
      <entry value="zoom"/>
      <entry value="fixed"/>
    </CssProperty>

    <CssProperty name="orientation">
      <entry value="auto"/>
      <entry value="landscape"/>
      <entry value="portrait"/>
    </CssProperty>
  </CssAtDirective>

  <CssPseudo name="::-ms-browse" _locID="ms-browse-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="파일 컨트롤=입력 형식의 찾아보기 단추에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh779844.aspx"/>

  <CssPseudo name="::-ms-check" _locID="ms-check-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="라디오 단추 입력 컨트롤 또는 확인란의 확인 표시에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465739.aspx"/>

  <CssPseudo name="::-ms-clear" _locID="ms-clear-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="내용이 없는 텍스트 입력 컨트롤의 입력 영역에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465740.aspx"/>

  <CssPseudo name="::-ms-expand" _locID="ms-expand-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="선택 컨트롤의 드롭다운 단추에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465742.aspx"/>

  <CssPseudo name="::-ms-fill" _locID="ms-fill-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="진행률 표시줄의 막대 부분에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465757.aspx"/>

  <CssPseudo name="::-ms-fill-lower" _locID="ms-fill-lower-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="슬라이더의 가장 작은 값에서 현재 위치 조정 컨트롤로 선택한 값까지 슬라이더 트랙 부분에 하나 이상의 스타일을 적용합니다. 왼쪽에서 오른쪽 레이아웃에서 이 부분은 위치 조정 컨트롤의 왼쪽에 있는 슬라이더 트랙 부분입니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465745.aspx"/>

  <CssPseudo name="::-ms-fill-upper" _locID="ms-fill-upper-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="현재 위치 조정 컨트롤로 선택한 값에서 슬라이더의 가장 큰 값까지 슬라이더 트랙 부분에 하나 이상의 스타일을 적용합니다. 왼쪽에서 오른쪽 레이아웃에서 이 부분은 위치 조정 컨트롤의 오른쪽에 있는 슬라이더 트랙 부분입니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465748.aspx"/>

  <CssPseudo name="::-ms-reveal" _locID="ms-reveal-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="암호 컨트롤=입력 형식의 암호 표시 단추에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465773.aspx"/>

  <CssPseudo name="::-ms-thumb" _locID="ms-thumb-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="사용자가 끌어가는 범위 입력 컨트롤(슬라이더 컨트롤이라고도 함) 부분에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465780.aspx"/>

  <CssPseudo name="::-ms-ticks-after" _locID="ms-ticks-after-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="위치 조정 컨트롤 바로 뒤에서 시작되어 슬라이더의 가장 큰 값까지 계속되는 슬라이더의 눈금 표시에 하나 이상의 스타일을 적용합니다. 왼쪽에서 오른쪽 레이아웃에서 이 눈금은 눈금 조정 컨트롤의 오른쪽에 있는 눈금입니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465789.aspx"/>

  <CssPseudo name="::-ms-ticks-before" _locID="ms-ticks-before-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="현재 위치 조정 컨트롤로 선택한 값까지 가장 작은 값을 나타내는 슬라이더의 눈금 표시에 하나 이상의 스타일을 적용합니다. 왼쪽에서 오른쪽 레이아웃에서 이 눈금은 눈금 조정 컨트롤의 왼쪽에 있는 눈금입니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465796.aspx"/>

  <CssPseudo name="::-ms-tooltip" _locID="ms-tooltip-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="슬라이더(입력 형식=범위)의 도구 설명에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465805.aspx"/>

  <CssPseudo name="::-ms-track" _locID="ms-track-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="슬라이더 트랙에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465813.aspx"/>

  <CssPseudo name="::-ms-value" _locID="ms-value-pseudo-element" _locAttrData="description" version="3.0" browsers="IE10" description="텍스트 또는 암호 입력 컨트롤이나 선택 컨트롤의 내용에 하나 이상의 스타일을 적용합니다." standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh465820.aspx"/>

  <CssPseudo name=":-ms-lang(en)" _locID="lang-pseudo-function" _locAttrData="description" version="3.0" browsers="IE10" syntax="html:-ms-lang(en) { color: blue; }" description="의사 클래스 :-ms-lang(C)는 C 언어에 있는 요소를 나타냅니다." standard-reference="http://www.w3.org/TR/css3-selectors/#lang-pseudo"/>

  <CssPseudo name=":-ms-input-placeholder" version="3.0" browsers="IE10" syntax="input:-ms-input-placeholder { color: red; }" standard-reference="http://msdn.microsoft.com/en-us/library/windows/apps/hh767367.aspx"/>

  <CssPseudo name=":-ms-keyboard-active" version="3.0" browsers="IE10" syntax="input:-ms-keyboard-active { background: red; }" standard-reference="#NoOnlineDocs"/>

  <CssPseudo name="::-ms-backdrop" version="3.0" browsers="IE11" syntax="*|*:fullscreen::backdrop { position: fixed; }" standard-reference="http://www.w3.org/TR/fullscreen/#::backdrop-pseudo-element"/>

  <CssPseudo name=":-ms-fullscreen" version="3.0" browsers="IE11" syntax="iframe:fullscreen { border: none; }" standard-reference="http://www.w3.org/TR/fullscreen/#:fullscreen-pseudo-class"/>

  <CssProperty _locID="zoom" _locAttrData="description" version="3.0" browsers="IE6,S4" restriction="enum, integer, number, percentage" syntax=".example { $(name): 1; }" description="개체 배율을 설정하거나 검색합니다." standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx">

    <entry value="normal"/>
  </CssProperty>

  <CssProperty name="-ms-zoom" ref="zoom" browsers="IE8"/>

  <CssProperty _locID="filter" _locAttrData="description" version="3.0" browsers="IE6-9" restriction="" syntax="div { $(name): alpha(opacity = 50); }" description="IE 전용입니다. 시각적 효과를 생성하는 데 사용됩니다." standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx"/>

  <CssProperty name="-ms-filter" ref="filter" restriction="" syntax="div { $(name): 'alpha(opacity = 50)'; }" browsers="IE8-9"/>

  <CssProperty _locID="behavior" _locAttrData="description" version="3.0" browsers="IE6" type="url" restriction="url" syntax="div { $(name): url(http://example.com/png_fix.htc); }" description="IE 전용입니다. 브라우저 동작을 확장하는 데 사용됩니다." standard-reference="http://msdn.microsoft.com/en-us/ie/gg192966.aspx">

    <entry value="none"/>
  </CssProperty>

  <CssProperty name="-ms-behavior" ref="behavior" browsers="IE8"/>

  <CssProperty _locID="-ms-accelerator" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="u { $(name): true; }" description="IE 전용입니다. Alt 키를 누를 때까지 액셀러레이터 키의 시스템 밑줄을 해제할 수 있습니다." standard-reference="http://www.css3.com/css-accelerator/">

    <entry _locID="-ms-accelerator_true" _locAttrData="description" value="true" description="요소에 액셀러레이터 키 시퀀스가 있습니다."/>
    <entry _locID="-ms-accelerator_false" _locAttrData="description" value="false" description="요소에 액셀러레이터 키 시퀀스가 포함되지 않습니다."/>
  </CssProperty>

  <CssProperty _locID="-ms-line-break" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="p { $(name): strict; }" description="요소에 적용할 줄 바꿈 제한 집합을 지정합니다." standard-reference="http://www.w3.org/TR/css3-text/#line-break0">

    <entry _locID="-ms-line-break_auto" _locAttrData="description" value="auto" description="UA는 CJK 스크립트에 사용할 줄 바꿈 제한 집합을 결정하며 줄 길이에 따라 제한을 변경할 수 있습니다. 줄이 짧으면 비교적 덜 제한적인 줄 바꿈 규칙 집합을 사용합니다."/>
    <entry _locID="-ms-line-break_newspaper" _locAttrData="description" value="newspaper" description="제한이 가장 적은 줄 바꿈 규칙 집합을 사용하여 CJK 스크립트 줄을 바꿉니다. 신문과 같은 짧은 줄에 주로 사용됩니다."/>
    <entry _locID="-ms-line-break_normal" _locAttrData="description" value="normal" description="표준 줄 바꿈 규칙 집합을 사용하여 CJK 스크립트 줄을 바꿉니다."/>
    <entry _locID="-ms-line-break_strict" _locAttrData="description" value="strict" description="'normal'보다 제한적인 줄 바꿈 규칙 집합을 사용하여 CJK 스크립트를 나눕니다."/>
    <entry _locID="-ms-line-break_keep-all" _locAttrData="description" value="keep-all" description="CJK 문자의 시퀀스는 암시적인 줄 바꿈 지점에서 더 이상 줄을 바꿀 수 없습니다. 이 옵션은 단어 한국어처럼 구분 기호 문자로 줄 바꿈 기회를 만드는 경우에만 사용해야 합니다."/>
  </CssProperty>

  <!-- Text -->

  <CssProperty _locID="-ms-text-overflow" ref="text-overflow" browsers="IE10"/>

  <CssProperty _locID="-ms-text-underline-position" _locAttrData="description" version="3.0" browsers="IE10" restriction="enum" syntax="div { $(name): auto; }" description="같은 요소에 지정된 밑줄의 위치를 설정합니다. 상위 요소가 지정된 밑줄에는 적용되지 않습니다. 이 속성은 대개 일본어 문서와 같이 해당 텍스트 줄 '위로'(오른쪽으로) 밑줄을 표시하려는 경우 세로 쓰기 컨텍스트에서 사용됩니다." standard-reference="http://www.w3.org/TR/css3-text/#text-underline-position0">

    <entry _locID="-ms-text-underline-position_auto" _locAttrData="description" value="auto" description="사용자 에이전트는 임의의 알고리즘을 사용하여 밑줄 위치를 결정할 수 있습니다. 가로 선 레이아웃에서는 밑줄이 글자에 맞춰지고 세로 선 레이아웃에서는 언어가 일본어나 한국어로 설정된 경우 밑줄이 위쪽에 맞춰집니다."/>
    <entry _locID="-ms-text-underline-position_under" _locAttrData="description" value="under" description="요소 em-box의 '아래쪽'(세로 쓰기에서는 왼쪽) 가장자리에 맞춰 정렬됩니다. 이 경우 밑줄은 대개 디센더와 교차하지 않으며 때때로 'accounting' 밑줄이라고도 합니다."/>
    <entry _locID="-ms-text-underline-position_alphabetic" _locAttrData="description" value="alphabetic" description="밑줄이 alphabetic 기준선에 맞춰 정렬됩니다. 이 경우 밑줄은 일부 디센더와 교차할 수 있습니다."/>
    <entry _locID="-ms-text-underline-position_over" _locAttrData="description" value="over" description="밑줄이 요소 em-box의 '위쪽'(세로 쓰기에서는 오른쪽) 가장자리에 맞춰 정렬됩니다. 이 모드에서는 윗줄이 양쪽을 전환하기도 합니다."/>
  </CssProperty>

