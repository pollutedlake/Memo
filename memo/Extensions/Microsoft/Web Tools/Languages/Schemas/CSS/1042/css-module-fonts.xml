<CssModule name="Fonts" standard-reference="http://www.w3.org/TR/css3-fonts/" status="WD" pub-date="2011-10-04">

  <CssAtDirective name="font-face" _locID="font-face-rule" _locAttrData="description" version="3.0" browsers="all" include-root-properties="false" include-root-directives="false" syntax="@font-face { &lt;font-description&gt; }" description="필요할 때 자동으로 활성화되는 글꼴에 대한 연결을 허용합니다. 이렇게 하면 작성자가 '웹에 적합한' 글꼴이 가지는 제한의 문제를 해결할 수 있어 특정 사용자의 환경에서 사용 가능한 글꼴에 상관없이 일관된 렌더링이 가능해집니다." standard-reference="http://www.w3.org/TR/css3-fonts/#the-font-face-rule">

    <CssProperty ref="font"/>
    <CssProperty _locID="font-face_font-family" _locAttrData="description" name="font-family" description="일치하는 모든 CSS 글꼴 패밀리 이름에서 사용될 글꼴 패밀리 이름을 정의합니다." syntax="@font-face { $(name): Gentium; }" standard-reference="http://www.w3.org/TR/css3-fonts/#descdef-font-family"/>
    <CssProperty ref="font-size"/>
    <CssProperty ref="font-variant"/>
    <CssProperty ref="font-weight"/>
    <CssProperty ref="font-style"/>
    <CssProperty ref="font-size-adjust"/>
    <CssProperty ref="font-stretch"/>
    <CssProperty ref="font-display"/>

    <CssProperty _locID="src" _locAttrData="description" version="3.0" restriction="enum, string, url, identifier" syntax="src: url(font.woff) format('woff');" description="글꼴 데이터를 포함하는 리소스를 지정합니다. 글꼴을 다운로드할 수 있는지, 아니면 글꼴이 로컬로 설치되는지에 관계없이 이 리소스가 필요합니다. 해당 값은 외부 참조 또는 로컬로 설치되는 글꼴 이름을 우선 순위에 따라 쉼표로 구분한 목록입니다." standard-reference="http://www.w3.org/TR/css3-fonts/#src-desc">

      <entry value="url()"/>
      <entry value="local()"/>
      <entry _locID="src_formatTT" _locAttrData="description" value="format('truetype')" description="트루타입 글꼴입니다."/>
      <entry _locID="src_formatOT" _locAttrData="description" value="format('opentype')" description="오픈타입 글꼴입니다."/>
      <entry _locID="src_formatWOFF" _locAttrData="description" value="format('woff')" description="WOFF(웹 개방형 글꼴 형식) 글꼴입니다."/>
      <entry _locID="src_formatEOT" _locAttrData="description" value="format('embedded-opentype')" description="포함된 오픈타입 글꼴입니다."/>
      <entry _locID="src_formatSVG" _locAttrData="description" value="format('svg')" description="SVG 글꼴입니다."/>
    </CssProperty>

    <CssProperty _locID="unicode-range" _locAttrData="description" version="3.0" restriction="unicode-range" description="특정 글꼴로 지원되는 유니코드 문자 범위입니다. 초기 값은 U+0-10FFFF입니다." syntax="'urange' [, 'urange']*" standard-reference="http://www.w3.org/TR/css3-fonts/#unicode-range"/>

  </CssAtDirective>

  <CssProperty _locID="font-weight" _locAttrData="description" version="1.0" restriction="enum" type="font-weight" syntax="th { $(name): bold; }" description="글꼴에 있는 문자 모양의 두께, 검은 정도 또는 스트로크 두께를 지정합니다." standard-reference="http://www.w3.org/TR/css3-fonts/#font-weight-the-font-weight-property"/>
    
  <CssPropertyValue type="font-weight">
    <entry _locID="font-weight_bold" _locAttrData="description" value="bold" description="700과 같습니다."/>
    <entry _locID="font-weight_bolder" _locAttrData="description" value="bolder" description="글꼴의 두께를 상속된 값보다 굵게 지정합니다."/>
    <entry _locID="font-weight_lighter" _locAttrData="description" value="lighter" description="글꼴의 두께를 상속된 값보다 얇게 지정합니다."/>
    <entry _locID="font-weight_normal" _locAttrData="description" value="normal" description="400과 같습니다."/>
    <entry _locID="font-weight_100" _locAttrData="description" value="100" description="가늘게"/>
    <entry _locID="font-weight_200" _locAttrData="description" value="200" description="아주 얇게"/>
    <entry _locID="font-weight_300" _locAttrData="description" value="300" description="얇게"/>
    <entry _locID="font-weight_400" _locAttrData="description" value="400" description="보통"/>
    <entry _locID="font-weight_500" _locAttrData="description" value="500" description="중간"/>
    <entry _locID="font-weight_600" _locAttrData="description" value="600" description="약간 굵게"/>
    <entry _locID="font-weight_700" _locAttrData="description" value="700" description="굵게"/>
    <entry _locID="font-weight_800" _locAttrData="description" value="800" description="아주 굵게"/>
    <entry _locID="font-weight_900" _locAttrData="description" value="900" description="검정(진하게)"/>
  </CssPropertyValue>

  <CssProperty _locID="font-stretch" _locAttrData="description" version="3.0" browsers="FF9,IE9" restriction="enum" syntax="div { $(name): expanded; }" description="font-stretch 속성은 글꼴의 가로 너비를 확장하거나 축소하는 데 사용됩니다. 변경은 브라우저에 표시되는 글꼴의 표준 너비에 상대적입니다." standard-reference="http://www.w3.org/TR/css3-fonts/#font-stretch0">

    <entry value="normal"/>
    <entry value="ultra-condensed"/>
    <entry value="extra-condensed"/>
    <entry value="condensed"/>
    <entry value="semi-condensed"/>
    <entry value="semi-expanded"/>
    <entry value="expanded"/>
    <entry value="extra-expanded"/>
    <entry value="ultra-expanded"/>
  </CssProperty>

  <CssProperty _locID="font-size" _locAttrData="description" version="1.0" restriction="length, percentage" type="font-size" description="글꼴에 있는 문자 모양의 원하는 높이를 나타냅니다. 확장 가능한 글꼴의 경우 font-size가 글꼴의 EM 단위에 적용되는 배율입니다. 해당 EM 상자 밖으로 누수되는 문자 모양도 있습니다. 확장 불가능한 글꼴의 경우에는 font-size가 절대 단위로 변환되고 일치된 값 모두에 대해 같은 절대 좌표 공간을 사용하여 글꼴의 선언된 font-size와 비교됩니다." syntax="div { $(name): 12px; }" standard-reference="http://www.w3.org/TR/css3-fonts/#font-size-prop"/>

  <CssPropertyValue type="font-size">
    <entry value="large"/>
    <entry value="larger"/>
    <entry value="medium"/>
    <entry value="small"/>
    <entry value="smaller"/>
    <entry value="x-large"/>
    <entry value="x-small"/>
    <entry value="xx-large"/>
    <entry value="xx-small"/>
  </CssPropertyValue>

  <CssProperty _locID="font-style" _locAttrData="description" version="1.0" restriction="enum" type="font-style" syntax="body { $(name): italic; }" description="기울임꼴 또는 오블리크 글꼴을 선택할 수 있습니다. 기울임꼴은 일반적으로 필기체의 특성을 띄지만 오블리크 글꼴은 일반적으로 보통 글꼴을 기울인 것입니다." standard-reference="http://www.w3.org/TR/css3-fonts/#font-style0"/>

  <CssPropertyValue type="font-style">
    <entry _locID="font-style_italic" _locAttrData="description" value="italic" description="레이블이 '기울임꼴'인 글꼴을 선택하거나 이 글꼴을 사용할 수 없는 경우 레이블이 '오블리크'인 글꼴을 선택합니다."/>
    <entry _locID="font-style_normal" _locAttrData="description" value="normal" description="'보통'으로 분류되는 글꼴을 선택합니다."/>
    <entry _locID="font-style_oblique" _locAttrData="description" value="oblique" description="레이블이 '오블리크'인 글꼴을 선택합니다."/>
  </CssPropertyValue>

  <CssProperty _locID="font-variant" _locAttrData="description" version="1.0" restriction="enum" type="font-variant" syntax="div { $(name): small-caps; }" description="글꼴의 변형 표현을 지정합니다." standard-reference="http://www.w3.org/TR/css3-fonts/#font-variant-prop"/>

  <CssPropertyValue type="font-variant">
    <entry _locID="font-variant_normal" _locAttrData="description" value="normal" description="레이블이 작은 대문자 글꼴이 아닌 글꼴을 지정합니다."/>
    <entry _locID="font-variant_small-caps" _locAttrData="description" value="small-caps" description="레이블이 작은 대문자 글꼴인 글꼴을 지정합니다. 진정한 작은 대문자 글꼴을 사용할 수 없는 경우 사용자 에이전트가 작은 대문자 글꼴을 시뮬레이션해야 합니다."/>
  </CssPropertyValue>

  <CssProperty _locID="font-family" _locAttrData="description" restriction="font" type="font" version="1.0" description="글꼴 패밀리 이름 또는 제네릭 패밀리 이름을 우선 순위에 따라 작성한 목록을 지정합니다. 사용자 에이전트는 렌더링할 문자의 문자 모양을 포함하는 사용 가능한 글꼴을 찾을 때까지 패밀리 이름 목록을 반복합니다." syntax="body { $(name): arial, verdana; }" standard-reference="http://www.w3.org/TR/css3-fonts/#font-family0"/>

  <CssPropertyValue type="font">
    <entry value="Arial, Helvetica, sans-serif"/>
    <entry value="Cambria, Cochin, Georgia, Times, Times New Roman, serif"/>
    <entry value="Courier New, Courier, monospace"/>
    <entry value="'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif"/>
    <entry value="'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif"/>
    <entry value="Georgia, 'Times New Roman', Times, serif"/>
    <entry value="'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif"/>
    <entry value="Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif"/>
    <entry value="'Segoe UI', Tahoma, Geneva, Verdana, sans-serif"/>
    <entry value="'Times New Roman', Times, serif"/>
    <entry value="'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif"/>
    <entry value="Verdana, Geneva, Tahoma, sans-serif"/>
    <entry value="serif"/>
    <entry value="sans-serif"/>
    <entry value="cursive"/>
    <entry value="fantasy"/>
    <entry value="monospace"/>
  </CssPropertyValue>

  <CssProperty _locID="font-size-adjust" _locAttrData="description" version="3.0" browsers="FF3.5,IE10" restriction="number" description="이 속성을 사용하면 글꼴 대체가 발생할 때 텍스트의 가독성을 유지할 수 있습니다. 이는 사용되는 글꼴에 상관없이 x-높이가 같도록 font-size를 조정하여 가능합니다." syntax="div { $(name): 0.58; }" standard-reference="http://www.w3.org/TR/css3-fonts/#font-size-adjust">

    <entry _locID="font-size-adjust_none" _locAttrData="description" value="none" description="글꼴의 x-높이를 유지하지 않습니다."/>
  </CssProperty>

  <CssProperty _locID="font" _locAttrData="description" version="1.0" restriction="font" type="font-weight, font-style, font-variant, font-size" description="'font-style', 'font-variant', 'font-weight', 'font-size', 'line-height' 및 'font-family'를 스타일시트의 같은 위치에서 설정하기 위한 줄임 속성입니다. 글꼴과 관련된 여러 속성을 설정하기 위해 이 속성의 구문은 기존의 인쇄 줄임 표기를 기반으로 합니다." syntax="body { $(name): bold 12px arial, verdana; }" standard-reference="http://www.w3.org/TR/css3-fonts/#propdef-font">

    <entry _locID="font_caption" _locAttrData="description" value="caption" version="3.0" description="캡션이 지정된 컨트롤(예: 단추, 드롭다운 등)에 사용되는 글꼴입니다."/>
    <entry _locID="font_icon" _locAttrData="description" value="icon" version="3.0" description="아이콘에 레이블을 지정하는 데 사용되는 글꼴입니다."/>
    <entry _locID="font_menu" _locAttrData="description" value="menu" version="3.0" description="메뉴(예: 드롭다운 메뉴 및 메뉴 목록)에 사용되는 글꼴입니다."/>
    <entry _locID="font_message-box" _locAttrData="description" value="message-box" version="3.0" description="대화 상자에 사용되는 글꼴입니다."/>
    <entry _locID="font_small-caption" _locAttrData="description" value="small-caption" version="3.0" description="작은 컨트롤에 레이블을 지정하는 데 사용되는 글꼴입니다."/>
    <entry _locID="font_status-bar" _locAttrData="description" value="status-bar" version="3.0" description="창의 상태 표시줄에 사용되는 글꼴입니다."/>
  </CssProperty>

  <CssProperty _locID="font-feature-settings" _locAttrData="description" version="3.0" browsers="IE10" restriction="string, integer" description="이 속성은 오픈타입 글꼴 기능에 대한 수준이 낮은 제어를 제공합니다. 이 속성은 널리 사용되지 않지만 특정 사용 사례에 필요한 글꼴 기능에 대한 액세스를 제공하기 위해 사용됩니다." syntax="body { $(name): 'hwid'; }" standard-reference="http://www.w3.org/TR/css3-fonts/#propdef-font-feature-settings">

    <entry _locID="font-feature-settings-normal" _locAttrData="description" value="normal" version="3.0" description="문자 모양 대체 또는 배치가 변경되지 않습니다."/>
    <entry value="on" browsers="none"/>
    <entry value="off" browsers="none"/>
    <entry value="&quot;dlig&quot;"/>
    <entry value="&quot;smcp&quot;"/>
    <entry value="&quot;c2cs&quot;"/>
    <entry value="&quot;liga&quot;"/>
    <entry value="&quot;lnum&quot;"/>
    <entry value="&quot;onum&quot;"/>
    <entry value="&quot;tnum&quot;"/>
    <entry value="&quot;kern&quot;"/>
    <entry value="&quot;swsh&quot;"/>
  </CssProperty>

  <CssProperty _locID="font-kerning" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="커닝은 문자 모양 간 간격을 컨텍스트에 맞게 조정하는 것입니다. 이 속성은 글꼴에 포함된 조정 데이터를 활용하는 커닝인 메트릭 커닝을 제어합니다." syntax="body { $(name): normal; }" standard-reference="http://www.w3.org/TR/css3-fonts/#propdef-font-kerning">
      
      <entry value="auto"/>
      <entry value="normal"/>
      <entry value="none"/>
  </CssProperty>

  <CssProperty _locID="font-language-override" _locAttrData="description" version="3.0" browsers="none" restriction="string" description="값 'normal'은 오픈타입 글꼴로 렌더링할 때 문서 언어를 사용하여 오픈타입 언어 시스템(렌더링 시 언어별 기능을 선택하는 데 사용됨)을 유추함을 의미합니다." syntax="body { $(name): 'SRB'; }" standard-reference="http://www.w3.org/TR/css3-fonts/#font-language-override-prop">

    <entry _locID="font-language-override_normal" _locAttrData="description" value="normal" description="오픈타입 글꼴로 렌더링할 때 문서 언어를 사용하여 오픈타입 언어 시스템(렌더링 시 언어별 기능을 선택하는 데 사용됨)을 유추함을 의미합니다."/>
  </CssProperty>

  <CssProperty _locID="font-synthesis" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="이 속성은 글꼴 패밀리에 굵은 글꼴 또는 기울임꼴이 없을 때 사용자 에이전트가 굵은 글꼴 또는 오블리크 글꼴을 만들 수 있는지 여부를 제어합니다." syntax="html:lang(ar) { $(name): none; }" standard-reference="http://www.w3.org/TR/css3-fonts/#propdef-font-synthesis">

    <entry _locID="font-synthesis_none" _locAttrData="description" value="none" description="모든 합성 글꼴을 허용하지 않습니다."/>
    <entry value="style"/>
    <entry value="weight"/>
  </CssProperty>

  <CssProperty _locID="font-variant-alternates" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="모든 문자에 대해 글꼴은 다양한 대체 문자 모양과 해당 문자의 기본 문자 모양을 제공할 수 있습니다. 이 속성을 사용하면 이러한 대체 문자 모양 선택 작업을 제어할 수 있습니다." syntax="h2 { $(name): styleset(3,5); }" standard-reference="http://www.w3.org/TR/css3-fonts/#propdef-font-variant-alternates">

    <entry value="normal"/>
    <entry _locID="font-variant-alternates_stylistic" _locAttrData="description" value="stylistic()" description="대체 스타일을 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_contextual" _locAttrData="description" value="contextual" description="대체 컨텍스트를 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_no-contextual" _locAttrData="description" value="no-contextual" description="대체 컨텍스트를 표시할 수 없습니다."/>
    <entry _locID="font-variant-alternates_historical-forms" _locAttrData="description" value="historical-forms" description="기록 형식을 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_styleset" _locAttrData="description" value="styleset()" description="스타일 집합을 사용하여 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_character-variant" _locAttrData="description" value="character-variant()" description="특정 변형 문자를 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_swash" _locAttrData="description" value="swash()" description="선단 장식 문자 모양을 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_ornaments" _locAttrData="description" value="ornaments()" description="기본 문자 모양을 장식(글꼴에 제공된 경우)으로 대체할 수 있습니다."/>
    <entry _locID="font-variant-alternates_annotation" _locAttrData="description" value="annotation()" description="대체 주석 형식을 표시할 수 있습니다."/>
    <entry _locID="font-variant-alternates_ruby" _locAttrData="description" value="ruby" description="Ruby 문자 모양을 표시할 수 있습니다. 이 값은 Ruby 텍스트에 사용할 문자 모양만 선택하며 글꼴 배율에는 영향을 주지 않습니다."/>
  </CssProperty>

  <CssProperty _locID="font-variant-caps" _locAttrData="description" version="3.0" browsers="none" restriction="enum" description="대문자 형식에 대한 제어를 지정합니다." syntax="p { $(name): titling-caps; }" standard-reference="http://www.w3.org/TR/css3-fonts/#font-variant-caps-prop">

